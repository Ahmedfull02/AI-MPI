   "source": [
    "from mpi4py import MPI\n",
    "\n",
    "# Initialize MPI\n",
    "comm = MPI.COMM_WORLD\n",
    "rank = comm.Get_rank()  # Process rank\n",
    "size = comm.Get_size()  # Total number of processes\n",
    "\n",
    "# Print \"Hello, World!\" from each process\n",
    "print(f\"Hello, World from rank {rank} out of {size}\")\n"
   ]
  }
